{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": "(auth|workspaces|projects|invites|tasks|comments|debug-tasks|cache).e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "testTimeout": 30000,
  "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"]
}
